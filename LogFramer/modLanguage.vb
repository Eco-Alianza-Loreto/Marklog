#Region "Methods"
    Public Sub ChangeLanguage_Variables(ByVal strLanguage As String)
        Select Case strLanguage
            Case "fr"
                With My.Settings
                    .setLanguage = strLanguage
                    .setStruct1sing = "Objectif général"
                    .setStruct2sing = "Objectif spécifique"
                    .setStruct3sing = "Résultat"
                    .setStruct4sing = "Activité"
                    .setStruct1 = "Objectifs généraux"
                    .setStruct2 = "Objectifs spécifiques"
                    .setStruct3 = "Résultats"
                    .setStruct4 = "Activités"
                    .setHelpPath = "Help\Logframer 2 Aide.chm"
                    .setHelpOnline = "http://www.logframer.eu/?language=fr"
                    .setHelpOnlineFacilidev = "http://www.facilidev.eu/"
                End With
            Case "nl"
                With My.Settings
                    .setLanguage = strLanguage
                    .setStruct1sing = "Algemene doelstelling"
                    .setStruct2sing = "Specifieke doelstelling"
                    .setStruct3sing = "Resultaat"
                    .setStruct4sing = "Activiteit"
                    .setStruct1 = "Algemene doelstellingen"
                    .setStruct2 = "Specifieke doelstellingen"
                    .setStruct3 = "Resultaten"
                    .setStruct4 = "Activiteiten"
                    .setHelpPath = "Help\Logframer 2 Help.chm"
                    .setHelpOnline = "http://www.logframer.eu/"
                    .setHelpOnlineFacilidev = "http://www.facilidev.eu/"
                End With
            Case Else
                With My.Settings
                    .setLanguage = strLanguage
                    .setStruct1sing = "Goal"
                    .setStruct2sing = "Purpose"
                    .setStruct3sing = "Output"
                    .setStruct4sing = "Activity"
                    .setStruct1 = "Goals"
                    .setStruct2 = "Purposes"
                    .setStruct3 = "Outputs"
                    .setStruct4 = "Activities"
                    .setHelpPath = "Help\Logframer 2 Help.chm"
                    .setHelpOnline = "http://www.logframer.eu/"
                    .setHelpOnlineFacilidev = "http://www.facilidev.eu/"
                End With
        End Select

        If CurrentLogFrame IsNot Nothing Then
            CurrentLogFrame.StructName = New String() {My.Settings.setStruct1sing, My.Settings.setStruct2sing, My.Settings.setStruct3sing, My.Settings.setStruct4sing}
            CurrentLogFrame.StructNamePlural = New String() {My.Settings.setStruct1, My.Settings.setStruct2, My.Settings.setStruct3, My.Settings.setStruct4}
            CurrentLogFrame.CreateSectionTitles()
        End If
    End Sub

