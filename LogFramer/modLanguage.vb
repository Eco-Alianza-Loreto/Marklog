            .Add(New StructuredComboBoxItem(LIST_QuestionTypes(30), False, False, 30)) 'Ranking
            .Add(New StructuredComboBoxItem(LIST_QuestionTypes(341), False, False, 341)) 'Likert-type scale
            .Add(New StructuredComboBoxItem(LIST_QuestionTypes(32), False, False, 32)) 'Simple semantic differential

            .Add(New StructuredComboBoxItem(LANG_ExpressingOpinion, True, False, CONST_QuestionTypeTitle))

            .Add(New StructuredComboBoxItem(LIST_QuestionTypes(33), False, False, 33)) 'Thurstone scale
            .Add(New StructuredComboBoxItem(LIST_QuestionTypes(34), False, False, 34)) 'Likert scale (summative scale)
            .Add(New StructuredComboBoxItem(LIST_QuestionTypes(35), False, False, 35)) 'Cumulative (Guttman) scale
            .Add(New StructuredComboBoxItem(LIST_QuestionTypes(36), False, False, 36)) 'Frequency table - Likert

            .Add(New StructuredComboBoxItem(LANG_OtherTypes, True, False, CONST_QuestionTypeTitle))

            .Add(New StructuredComboBoxItem(LIST_QuestionTypes(61), False, False, 61)) 'Images with targets

            .Add(New StructuredComboBoxItem(LANG_SubIndicatorOfDifferentType, True, False, CONST_QuestionTypeTitle))
